<UserControl x:Class="AutomationControls.communication.mqtt.AsrTokenDataControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
			 xmlns:convert="clr-namespace:AutomationControls.WPF.Converters;assembly=PD_ClassLibrary"
             xmlns:local="clr-namespace:AutomationControls.communication.mqtt">
    <StackPanel Orientation="Vertical" >
	   <DockPanel DockPanel.Dock="Top">
<TextBlock Text="value" /><TextBox Text="{Binding value}" /></DockPanel>

<DockPanel DockPanel.Dock="Top">
<TextBlock Text="confidence" /><TextBox Text="{Binding confidence}" /></DockPanel>

<DockPanel DockPanel.Dock="Top">
<TextBlock Text="range_start" /><TextBox Text="{Binding range_start}" /></DockPanel>

<DockPanel DockPanel.Dock="Top">
<TextBlock Text="range_end" /><TextBox Text="{Binding range_end}" /></DockPanel>


    </StackPanel>
</UserControl>